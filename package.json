    "styled-components": "^3.3.0",
    "styled-system": "^2.2.5"
  },
  "x0": {
    "title": "Compositor x0",
    "basename": "/x0",
    "meta": [
      {
        "name": "description",
        "content": "Zero-config React development environment and static site generator"
      },
      {
        "name": "twitter:card",
        "content": "summary"
      },
      {
        "name": "twitter:site",
        "content": "@getcompositor"
      },
      {
        "name": "twitter:image",
        "content": "https://compositor.io/logo/dist/compositor.png"
      },
      {
        "name": "twitter:title",
        "content": "Compositor x0"
      },
      {
        "name": "twitter:description",
        "content": "Zero-config React development environment and static site generator"
      }
    ]
  },
  "ava": {
    "files": [
      "test/*",
      "!test/components",
      "!test/output"
    ],
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
