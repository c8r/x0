  },
  "scripts": {
    "start": "./cli.js docs -op 8888",
    "build": "./cli.js build docs",
    "test": "ava -T 20s",
    "cover": "nyc report --reporter=html --reporter=lcov"
  },
  "keywords": [
    "react",
    "webpack",
    "zero-config",
